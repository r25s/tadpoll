    "express": "^4.13.4",
    "jquery": "^2.2.0",
    "randomcolor": "^0.4.3",
    "rethinkdb": "^2.2.1",
    "shortid": "^2.2.4",
    "socket.io": "^1.4.5",
    "vue": "^1.0.13",
    "vue-router": "^0.7.7",
    "vuex": "^0.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.2.3",
    "autoprefixer-loader": "^3.1.0",
